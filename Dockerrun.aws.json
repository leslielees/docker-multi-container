{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions" : [
        {
            "name" : "frontend",
            "image" :"leslielees/docker-multi-frontend",
            "hostname" :"frontend",
            "essential" : false,
            "memory":128
        },
        {
            "name" : "api",
            "image" :"leslielees/docker-multi-api",
            "hostname" :"api",
            "essential" : false,
            "memory":128
        },
        {
            "name" : "worker",
            "image" :"leslielees/docker-multi-worker",
            "hostname" :"worker",
            "essential" : false,
            "memory":128
        },
        {
            "name" : "nginx",
            "image" :"leslielees/docker-multi-nginx",
            "hostname" :"nginx",
            "essential" : true,
            "portMapping":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["frontend","api"],
            "memory":128
        }
        
    ]
}